PROCEDURE more_complex_sync_test () return BOOL
DO
	VAR a, b, c AS BOOL;

	SYNC
		if a == TRUE
		THEN
			b = TRUE;
		ELSE
			c = FALSE;
		END
		a = NULL;
	ENDSYNC
ENDPROC

